package com.sskey.dms.model;

public class BalanceSum {
    private String iD;
    private String salesDate;
    private String salesMan;
    private double totalBalance;
    private double inputDailyAmount;

    public BalanceSum() {
    }

    public BalanceSum(String IDBS, String salesDate, String salesMan, String totalBalance, String inputDailyAmount) {
        this.iD = IDBS;
        salesDate = salesDate;
        salesMan = salesMan;
        totalBalance = totalBalance;
        inputDailyAmount = inputDailyAmount;
    }

    public String getIDBS() {
        return iD;
    }

    public void setIDBS(String IDBS) {
        this.iD= IDBS;
    }

    public String getSalesDate() {
        return salesDate;
    }

    public void setSalesDate(String salesDate) {
        salesDate = salesDate;
    }

    public String getSalesMan() {
        return salesMan;
    }

    public void setSalesMan(String salesMan) {
        salesMan = salesMan;
    }

    public double getTotalBalance() {
        return totalBalance;
    }

    public void setTotalBalance(double totalBalance) {
        totalBalance = totalBalance;
    }

    public double getInputDailyAmount() {
        return inputDailyAmount;
    }

    public void setInputDailyAmount(double inputDailyAmount) {
        inputDailyAmount = inputDailyAmount;
    }
}
